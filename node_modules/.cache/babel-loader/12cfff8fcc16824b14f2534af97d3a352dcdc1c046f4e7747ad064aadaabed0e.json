{"ast":null,"code":"import { reactive, toRefs } from 'vue';\nimport Docker from '../../components/Docker.vue';\nimport { get } from '../../utils/request';\nconst useOrderEffect = () => {\n  const data = reactive({\n    list: []\n  });\n  const getOrderData = async () => {\n    const result = await get('/api/order');\n    const orders = result.data.data;\n    let total = 0;\n    let count = 0;\n    orders.forEach(order => {\n      const product = order.products;\n      product.forEach(i => {\n        count += i.orderSales;\n        total += i.orderSales * i.product.price;\n      });\n      order.total = total;\n      order.count = count;\n    });\n    data.list = result.data.data;\n  };\n  getOrderData();\n  const {\n    list\n  } = toRefs(data);\n  return {\n    list\n  };\n};\nexport default {\n  name: 'OrderList',\n  components: {\n    Docker\n  },\n  setup() {\n    const {\n      list,\n      count,\n      total\n    } = useOrderEffect();\n    console.log(list);\n    return {\n      list,\n      count,\n      total\n    };\n  }\n};","map":{"version":3,"names":["reactive","toRefs","Docker","get","useOrderEffect","data","list","getOrderData","result","orders","total","count","forEach","order","product","products","i","orderSales","price","name","components","setup","console","log"],"sources":["C:\\Users\\21112\\Desktop\\前端啊啊啊\\jingdon\\src\\views\\orderList\\OrderList.vue"],"sourcesContent":["<template>\r\n  <div class=\"wrapper\">\r\n    <div class=\"title\">我的订单</div>\r\n    <div class=\"orders\">\r\n      <div class=\"order\" v-for=\"(item,index) in list\" :key=\"index\">\r\n        <div class=\"order__title\">{{item.shopName}}</div>\r\n        <span class=\"order__status\">已完成</span>\r\n        <div class=\"order__content\">\r\n          <div class=\"order__content__imgs\">\r\n            <img\r\n              class=\"order__content__img\"\r\n              v-for=\"(product,i) in item.products\"\r\n              :key=\"i\"\r\n              :src=\"product.product.img\"\r\n            />\r\n          </div>\r\n          <div class=\"order__content__info\">\r\n            <div class=\"order__content__price\">&yen;{{item.total}}</div>\r\n            <div class=\"order__content__count\">共{{item.count}}件</div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n  <Docker :currentIndex=\"2\" />\r\n</template>\r\n\r\n<script>\r\nimport { reactive, toRefs } from 'vue'\r\nimport Docker from '../../components/Docker.vue'\r\nimport { get } from '../../utils/request'\r\n\r\nconst useOrderEffect = () => {\r\n  const data = reactive({ list: [] })\r\n  const getOrderData = async () => {\r\n    const result = await get('/api/order')\r\n    const orders = result.data.data\r\n    let total = 0\r\n    let count = 0\r\n    orders.forEach((order) => {\r\n      const product = order.products\r\n      product.forEach((i) => {\r\n        count += i.orderSales\r\n        total += (i.orderSales * i.product.price)\r\n      })\r\n      order.total = total\r\n      order.count = count\r\n    })\r\n    data.list = result.data.data\r\n  }\r\n  getOrderData()\r\n  const { list } = toRefs(data)\r\n  return { list }\r\n}\r\nexport default {\r\n  name: 'OrderList',\r\n  components: { Docker },\r\n  setup () {\r\n    const { list, count, total } = useOrderEffect()\r\n    console.log(list)\r\n    return { list, count, total }\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang='scss' scoped>\r\n.wrapper {\r\n  overflow-y: auto;\r\n  position: absolute;\r\n  left: 0;\r\n  right: 0;\r\n  top: 0;\r\n  bottom: 0.5rem;\r\n  background: rgb(248, 248, 248);\r\n}\r\n.title {\r\n  line-height: 0.44rem;\r\n  text-align: center;\r\n  font-size: 0.14rem;\r\n  color: #333;\r\n  background: #fff;\r\n}\r\n.orders {\r\n}\r\n.order {\r\n  margin: 0.16rem 0.18rem;\r\n  padding: 0.16rem;\r\n  background: #fff;\r\n  &__title {\r\n    margin-bottom: 0.16rem;\r\n    line-height: 0.22rem;\r\n    font-size: 0.16rem;\r\n    color: #333;\r\n  }\r\n  &__status {\r\n    text-align: right;\r\n    float: right;\r\n    font-size: 0.14rem;\r\n    color: #999;\r\n  }\r\n  &__content {\r\n    display: flex;\r\n    &__imgs {\r\n      flex: 1;\r\n    }\r\n    &__img {\r\n      margin-right: .12rem;\r\n      width: 0.4rem;\r\n      height: 0.4rem;\r\n    }\r\n    &__info {\r\n        width: .7rem;\r\n    }\r\n    &__price {\r\n        line-height: .2rem;\r\n        font-size: .14rem;\r\n        color: #c92929;\r\n    }\r\n    &__count {\r\n        font-size: .12rem;\r\n        color: #999;\r\n    }\r\n  }\r\n}\r\n</style>\r\n"],"mappings":"AA4BA,SAASA,QAAQ,EAAEC,MAAK,QAAS,KAAI;AACrC,OAAOC,MAAK,MAAO,6BAA4B;AAC/C,SAASC,GAAE,QAAS,qBAAoB;AAExC,MAAMC,cAAa,GAAIA,CAAA,KAAM;EAC3B,MAAMC,IAAG,GAAIL,QAAQ,CAAC;IAAEM,IAAI,EAAE;EAAG,CAAC;EAClC,MAAMC,YAAW,GAAI,MAAAA,CAAA,KAAY;IAC/B,MAAMC,MAAK,GAAI,MAAML,GAAG,CAAC,YAAY;IACrC,MAAMM,MAAK,GAAID,MAAM,CAACH,IAAI,CAACA,IAAG;IAC9B,IAAIK,KAAI,GAAI;IACZ,IAAIC,KAAI,GAAI;IACZF,MAAM,CAACG,OAAO,CAAEC,KAAK,IAAK;MACxB,MAAMC,OAAM,GAAID,KAAK,CAACE,QAAO;MAC7BD,OAAO,CAACF,OAAO,CAAEI,CAAC,IAAK;QACrBL,KAAI,IAAKK,CAAC,CAACC,UAAS;QACpBP,KAAI,IAAMM,CAAC,CAACC,UAAS,GAAID,CAAC,CAACF,OAAO,CAACI,KAAK;MAC1C,CAAC;MACDL,KAAK,CAACH,KAAI,GAAIA,KAAI;MAClBG,KAAK,CAACF,KAAI,GAAIA,KAAI;IACpB,CAAC;IACDN,IAAI,CAACC,IAAG,GAAIE,MAAM,CAACH,IAAI,CAACA,IAAG;EAC7B;EACAE,YAAY,CAAC;EACb,MAAM;IAAED;EAAK,IAAIL,MAAM,CAACI,IAAI;EAC5B,OAAO;IAAEC;EAAK;AAChB;AACA,eAAe;EACba,IAAI,EAAE,WAAW;EACjBC,UAAU,EAAE;IAAElB;EAAO,CAAC;EACtBmB,KAAIA,CAAA,EAAK;IACP,MAAM;MAAEf,IAAI;MAAEK,KAAK;MAAED;IAAM,IAAIN,cAAc,CAAC;IAC9CkB,OAAO,CAACC,GAAG,CAACjB,IAAI;IAChB,OAAO;MAAEA,IAAI;MAAEK,KAAK;MAAED;IAAM;EAC9B;AACF"},"metadata":{},"sourceType":"module","externalDependencies":[]}