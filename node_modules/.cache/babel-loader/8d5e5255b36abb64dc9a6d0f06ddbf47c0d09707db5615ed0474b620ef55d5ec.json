t = 0\r\n      if (product) {\r\n        for (const i in product) {\r\n          const item = product[i]\r\n          count += item.count\r\n        }\r\n      }\r\n      return count\r\n    })\r\n    return { total }\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n.cart {\r\n    position: absolute;\r\n    left: 0;\r\n    right: 0;\r\n    bottom: 0;\r\n    height: .5rem;\r\n}\r\n.check {\r\n    display: flex;\r\n    height: .49rem;\r\n    line-height: .5rem;\r\n    &__icon {\r\n        position: relative;\r\n        height: .49rem;\r\n        width: .84rem;\r\n        &__img {\r\n            display: block;\r\n            margin: .28rem auto;\r\n            width: .28rem;\r\n            height: .26rem;\r\n        }\r\n        &__tag {\r\n            position: absolute;\r\n            right: .2rem;\r\n            top: .04rem;\r\n            width: .2rem;\r\n            height: .2rem;\r\n            line-height: .2rem;\r\n            text-align: center;\r\n            border-radius: 50%;\r\n            background-color: rgba(231, 61, 27, 0.977);\r\n        }\r\n    }\r\n    &__info {\r\n        flex: 1;\r\n        font-size: .12rem;\r\n        &__price {\r\n            font-size: .18rem;\r\n            color: rgba(230, 67, 17, 0.818);\r\n        }\r\n    }\r\n    &__btn {\r\n        width: .98rem;\r\n        background-color: rgb(58, 143, 223);\r\n        font-size: .18rem;\r\n        text-align: center;\r\n    }\r\n}\r\n</style>\r\n"],"mappings":"AAkBA,SAASA,QAAO,QAAS,YAAW;AACpC,SAASC,QAAO,QAAS,MAAK;AAC9B,SAASC,QAAO,QAAS,KAAI;AAE7B,eAAe;EACb;EACAC,IAAI,EAAE,MAAM;EACZC,KAAIA,CAAA,EAAK;IACP,MAAMC,KAAI,GAAIJ,QAAQ,CAAC;IACvB,MAAMK,KAAI,GAAIN,QAAQ,CAAC;IACvB,MAAMO,QAAO,GAAIF,KAAK,CAACG,KAAK,CAACD,QAAO;IACpC,MAAME,MAAK,GAAIH,KAAK,CAACI,MAAM,CAACC,EAAC;IAC7B,MAAMC,KAAI,GAAIV,QAAQ,CAAC,MAAM;MAC3B,MAAMW,OAAM,GAAIN,QAAQ,CAACE,MAAM;MAC/B,IAAIK,KAAI,GAAI;MACZ,IAAID,OAAO,EAAE;QACX,KAAK,MAAME,CAAA,IAAKF,OAAO,EAAE;UACvB,MAAMG,IAAG,GAAIH,OAAO,CAACE,CAAC;UACtBD,KAAI,I