{"ast":null,"code":"import { useRoute } from 'vue-router';\nimport { useStore } from 'vuex';\nimport { computed } from 'vue';\nexport default {\n  // eslint-disable-next-line vue/multi-word-component-names\n  name: 'Cart',\n  setup() {\n    const store = useStore();\n    const route = useRoute();\n    const cartList = store.state.cartList;\n    const shopId = route.params.id;\n    const total = computed(() => {\n      const product = cartList[shopId];\n      let count = 0;\n      if (product) {\n        for (const i in product) {\n          const item = product[i];\n          count += item.count;\n        }\n      }\n      return count;\n    });\n    const price = computed(() => {\n      const product = cartList[shopId];\n      let count = 0;\n      if (product) {\n        for (const i in product) {\n          const item = product[i];\n          if (item.check) {\n            count += item.count * item.price;\n          }\n        }\n      }\n      return count.toFixed(2);\n    });\n    const productList = computed(() => {\n      const productList = cartList[shopId] || [];\n      return productList;\n    });\n    const addItemToCart = (shopId, productId, productInfo, num) => {\n      store.commit('addItemToCart', {\n        shopId,\n        productId,\n        productInfo,\n        num\n      });\n    };\n    const checkIcon = (shopId, productId) => {\n      store.commit('checkIcon', {\n        shopId,\n        productId\n      });\n    };\n    return {\n      total,\n      price,\n      productList,\n      shopId,\n      addItemToCart,\n      checkIcon\n    };\n  }\n};","map":{"version":3,"names":["useRoute","useStore","computed","name","setup","store","route","cartList","state","shopId","params","id","total","product","count","i","item","price","check","toFixed","productList","addItemToCart","productId","productInfo","num","commit","checkIcon"],"sources":["C:\\Users\\21112\\Desktop\\前端啊啊啊\\jingdon\\src\\views\\shop\\cart.vue"],"sourcesContent":["<template>\r\n  <div class=\"cart\">\r\n    <div class=\"product\">\r\n      <template  v-for=\"item in productList\" :key=\"item._id\">\r\n        <div class=\"product__header\">\r\n          <div class=\"product__header__all\">全选</div>\r\n          <div class=\"product__header__clear\">清空购物车</div>\r\n        </div>\r\n        <div v-if=\"item.count > 0\" class=\"product__item\">\r\n          <div class=\"product__item__checked iconfont\"\r\n               v-html=\"item.check ? `&#xe70f;`:`&#xe66c;`\"\r\n               @click=\"() => checkIcon(shopId,item._id)\"\r\n          ></div>\r\n          <img :src='item.imgUrl'\r\n               class=\"product__item__img\" />\r\n          <div class=\"product__item__detail\">\r\n            <h4 class=\"product__item_title\">{{ item.name }}</h4>\r\n            <div class=\"product__item__price\">\r\n              <span class=\"product__item__yen\">&yen;</span>{{ item.price }}\r\n              <span class=\"product__item__origin\">&yen;{{ item.oldPrice }}</span>\r\n            </div>\r\n          </div>\r\n          <div class=\"product__number\">\r\n            <span class=\"product__number__minus\" @click=\"addItemToCart(shopId,item._id,item,-1)\">-</span>\r\n            {{item.count || 0}}\r\n            <span class=\"product__number__plus\"\r\n                  @click= \"() => { addItemToCart(shopId,item._id,item,1) }\"\r\n            >+</span>\r\n          </div>\r\n        </div>\r\n      </template>\r\n    </div>\r\n    <div class=\"check\">\r\n        <div class=\"check__icon\">\r\n            <img src=\"http://www.dell-lee.com/imgs/vue3/basket.png\"\r\n            class=\"check__icon__img\"\r\n            />\r\n            <div class=\"check__icon__tag\">{{total}}</div>\r\n        </div>\r\n        <div class=\"check__info\">\r\n            总计：<span class=\"check__info__price\">&yen;{{price}}</span>\r\n        </div>\r\n        <div class=\"check__btn\">去结算</div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { useRoute } from 'vue-router'\r\nimport { useStore } from 'vuex'\r\nimport { computed } from 'vue'\r\n\r\nexport default {\r\n  // eslint-disable-next-line vue/multi-word-component-names\r\n  name: 'Cart',\r\n  setup () {\r\n    const store = useStore()\r\n    const route = useRoute()\r\n    const cartList = store.state.cartList\r\n    const shopId = route.params.id\r\n    const total = computed(() => {\r\n      const product = cartList[shopId]\r\n      let count = 0\r\n      if (product) {\r\n        for (const i in product) {\r\n          const item = product[i]\r\n          count += item.count\r\n        }\r\n      }\r\n      return count\r\n    })\r\n    const price = computed(() => {\r\n      const product = cartList[shopId]\r\n      let count = 0\r\n      if (product) {\r\n        for (const i in product) {\r\n          const item = product[i]\r\n          if (item.check) {\r\n            count += (item.count * item.price)\r\n          }\r\n        }\r\n      }\r\n      return count.toFixed(2)\r\n    })\r\n    const productList = computed(() => {\r\n      const productList = cartList[shopId] || []\r\n      return productList\r\n    })\r\n    const addItemToCart = (shopId, productId, productInfo, num) => {\r\n      store.commit('addItemToCart', { shopId, productId, productInfo, num })\r\n    }\r\n    const checkIcon = (shopId, productId) => {\r\n      store.commit('checkIcon', { shopId, productId })\r\n    }\r\n    return { total, price, productList, shopId, addItemToCart, checkIcon }\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n.cart {\r\n    position: absolute;\r\n    left: 0;\r\n    right: 0;\r\n    bottom: 0;\r\n    height: .5rem;\r\n}\r\n.product {\r\n  overflow-y: scroll;\r\n  flex: 1;\r\n  background: #fff;\r\n  &__header {\r\n    display: flex;\r\n    &__all {\r\n      width: .64rem;\r\n      font-size: .14rem;\r\n    }\r\n    &__clear {\r\n      margin-right: .16rem;\r\n      font-size: .14rem;\r\n      text-align: right;\r\n      flex: 1;\r\n    }\r\n  }\r\n  &__item {\r\n    position: relative;\r\n    display: flex;\r\n    padding: .12rem 0;\r\n    margin: 0 .16rem;\r\n    border-bottom: .01rem solid #f1f1f1;\r\n    &__checked {\r\n      line-height: .5rem;\r\n      margin-right: .2rem;\r\n      font-size: .2rem;\r\n      color: #52b7ed;\r\n    }\r\n    &__img {\r\n      width: .46rem;\r\n      height: .46rem;\r\n      margin-right: .16rem;\r\n    }\r\n    &__title{\r\n      margin: 0;\r\n      line-height: .2rem;\r\n      font-size: .14rem;\r\n      color: #333;\r\n    }\r\n    &__price {\r\n      margin: .06rem 0 0 0;\r\n      line-height: .2rem;\r\n      font-size: .14rem;\r\n    }\r\n    &__yen {\r\n      font-size: .12rem;\r\n    }\r\n    &__origin {\r\n      margin-left: .06rem;\r\n      line-height: .2rem;\r\n      text-decoration: line-through;\r\n      color:#999;\r\n      font-size: .12rem;\r\n    }\r\n  }\r\n  &__number {\r\n    position:absolute;\r\n    right:  0;\r\n    bottom: .12rem;\r\n    &__minus,&__plus {\r\n      display: inline-block;\r\n      line-height: .16rem;\r\n      width: .2rem;\r\n      height: .2rem;\r\n      font-size: .2rem;\r\n      border-radius: 50%;\r\n      text-align: center;\r\n    }\r\n    &__minus {\r\n      border: .01rem solid #999;\r\n      margin-right: .1rem;\r\n    }\r\n    &__plus {\r\n      margin-left: .1rem;\r\n      background: #52b7ed;\r\n    }\r\n  }\r\n}\r\n.check {\r\n    display: flex;\r\n    height: .49rem;\r\n    line-height: .5rem;\r\n    &__icon {\r\n        position: relative;\r\n        height: .49rem;\r\n        width: .84rem;\r\n        &__img {\r\n            display: block;\r\n            margin: .28rem auto;\r\n            width: .28rem;\r\n            height: .26rem;\r\n        }\r\n        &__tag {\r\n            position: absolute;\r\n            left: .3rem;\r\n            right: .2rem;\r\n            top: .04rem;\r\n            min-width: .2rem;\r\n            height: .2rem;\r\n            line-height: .2rem;\r\n            text-align: center;\r\n            border-radius: .1rem;\r\n            background-color: rgba(231, 61, 27, 0.977);\r\n            transform: scale(0.7);\r\n            transform-origin: left center;\r\n            white-space: nowrap;\r\n            overflow: hidden;\r\n            text-overflow: ellipsis;\r\n        }\r\n    }\r\n    &__info {\r\n        flex: 1;\r\n        font-size: .12rem;\r\n        &__price {\r\n            font-size: .18rem;\r\n            color: rgba(230, 67, 17, 0.818);\r\n        }\r\n    }\r\n    &__btn {\r\n        width: .98rem;\r\n        background-color: rgb(58, 143, 223);\r\n        font-size: .18rem;\r\n        text-align: center;\r\n    }\r\n}\r\n</style>\r\n"],"mappings":"AAgDA,SAASA,QAAO,QAAS,YAAW;AACpC,SAASC,QAAO,QAAS,MAAK;AAC9B,SAASC,QAAO,QAAS,KAAI;AAE7B,eAAe;EACb;EACAC,IAAI,EAAE,MAAM;EACZC,KAAIA,CAAA,EAAK;IACP,MAAMC,KAAI,GAAIJ,QAAQ,CAAC;IACvB,MAAMK,KAAI,GAAIN,QAAQ,CAAC;IACvB,MAAMO,QAAO,GAAIF,KAAK,CAACG,KAAK,CAACD,QAAO;IACpC,MAAME,MAAK,GAAIH,KAAK,CAACI,MAAM,CAACC,EAAC;IAC7B,MAAMC,KAAI,GAAIV,QAAQ,CAAC,MAAM;MAC3B,MAAMW,OAAM,GAAIN,QAAQ,CAACE,MAAM;MAC/B,IAAIK,KAAI,GAAI;MACZ,IAAID,OAAO,EAAE;QACX,KAAK,MAAME,CAAA,IAAKF,OAAO,EAAE;UACvB,MAAMG,IAAG,GAAIH,OAAO,CAACE,CAAC;UACtBD,KAAI,IAAKE,IAAI,CAACF,KAAI;QACpB;MACF;MACA,OAAOA,KAAI;IACb,CAAC;IACD,MAAMG,KAAI,GAAIf,QAAQ,CAAC,MAAM;MAC3B,MAAMW,OAAM,GAAIN,QAAQ,CAACE,MAAM;MAC/B,IAAIK,KAAI,GAAI;MACZ,IAAID,OAAO,EAAE;QACX,KAAK,MAAME,CAAA,IAAKF,OAAO,EAAE;UACvB,MAAMG,IAAG,GAAIH,OAAO,CAACE,CAAC;UACtB,IAAIC,IAAI,CAACE,KAAK,EAAE;YACdJ,KAAI,IAAME,IAAI,CAACF,KAAI,GAAIE,IAAI,CAACC,KAAK;UACnC;QACF;MACF;MACA,OAAOH,KAAK,CAACK,OAAO,CAAC,CAAC;IACxB,CAAC;IACD,MAAMC,WAAU,GAAIlB,QAAQ,CAAC,MAAM;MACjC,MAAMkB,WAAU,GAAIb,QAAQ,CAACE,MAAM,KAAK,EAAC;MACzC,OAAOW,WAAU;IACnB,CAAC;IACD,MAAMC,aAAY,GAAIA,CAACZ,MAAM,EAAEa,SAAS,EAAEC,WAAW,EAAEC,GAAG,KAAK;MAC7DnB,KAAK,CAACoB,MAAM,CAAC,eAAe,EAAE;QAAEhB,MAAM;QAAEa,SAAS;QAAEC,WAAW;QAAEC;MAAI,CAAC;IACvE;IACA,MAAME,SAAQ,GAAIA,CAACjB,MAAM,EAAEa,SAAS,KAAK;MACvCjB,KAAK,CAACoB,MAAM,CA